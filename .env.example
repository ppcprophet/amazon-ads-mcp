# PPC Prophet MCP Server Configuration

# Laravel API URL (where your ppcpl9b app is running)
PPC_PROPHET_API_URL=http://localhost:8000

# Sanctum API Token (generate via: php73 artisan tinker -> $user->createToken('mcp')->plainTextToken)
PPC_PROPHET_API_KEY=your-sanctum-token-here

# Optional: Default profile ID to use when not specified
PPC_PROPHET_DEFAULT_PROFILE_ID=
